{
    "name": "@foboss/nestjs-env-getter",
    "version": "0.0.0-b1",
    "description": "Environment variables getter for Nestjs applications",
    "author": "Foboss",
    "private": false,
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Foboss6/nestjs-env-getter.git"
    },
    "license": "MIT",
    "scripts": {
        "prebuild": "rm -rf dist",
        "build": "tsc  --p tsconfig.build.json",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "lint": "eslint --fix",
        "lint:report": "eslint ",
        "test": "jest",
        "test:report": "jest --ci --reporters=default --reporters=jest-junit --reporters=jest-sonar --coverage --runInBand",
        "typecheck": "tsc  --noEmit"
    },
    "peerDependencies": {
        "@nestjs/common": ">=9"
    },
    "dependencies": {
        "dotenv": "16.4.7"
    },
    "devDependencies": {
        "@eslint/js": "^9.18.0",
        "@nestjs/common": "^9.4.2",
        "@nestjs/core": "^9.4.2",
        "@nestjs/testing": "^9.4.2",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.10.10",
        "eslint": "^9.18.0",
        "eslint-plugin-jsdoc": "^50.6.2",
        "globals": "^15.14.0",
        "jest": "^29.7.0",
        "jest-junit": "^16.0.0",
        "ts-jest": "^29.2.5",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.21.0"
    }
}
