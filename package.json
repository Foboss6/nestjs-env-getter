{
  "name": "@foboss/nestjs-env-getter",
  "version": "0.1.0",
  "description": "Environment variables getter for Nestjs applications",
  "author": "Foboss",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/index.js",
    "dist/index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Foboss6/nestjs-env-getter.git"
  },
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint-report": "eslint-output",
    "test": "jest",
    "test-report": "jest --ci --reporters=default --reporters=jest-junit --reporters=jest-sonar --coverage --runInBand",
    "typecheck": "tsc --p tsconfig.build.json --noEmit"
  },
  "peerDependencies": {
    "@nestjs/common": ">=9"
  },
  "dependencies": {
    "dotenv": "16.1.4"
  },
  "devDependencies": {
    "@nestjs/common": "^9.4.2",
    "@nestjs/core": "^9.4.2",
    "@nestjs/testing": "^9.4.2",
    "@rollup/plugin-commonjs": "^25.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/jest": "^29.5.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-formatter-gitlab": "^4.0.0",
    "jest": "^29.5.0",
    "reflect-metadata": "^0.1.13",
    "rollup": "^3.25.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rxjs": "^7.8.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  }
}
